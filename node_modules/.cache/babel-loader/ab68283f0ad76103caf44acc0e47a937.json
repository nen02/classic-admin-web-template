ion: function () {\r\n      return this.isTreeViewToggled ? { transform: 'rotate(-90deg)' } : { transform: '' }\r\n    }\r\n  },\r\n  watch: {\r\n    watcher: function () {\r\n      if (this.isTreeViewToggled) this.toggleTreeView()\r\n    }\r\n  }\r\n}\r\n</script>\r\n"],"sourceRoot":"src/components"}]}